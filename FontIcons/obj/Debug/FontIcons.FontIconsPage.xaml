		xmlns:local="clr-namespace:FontIcons" x:Class="FontIcons.FontIconsPage">

	<ContentPage.Content>

 
 <Grid x:Name="MainGrid" VerticalOptions="FillAndExpand" Padding="0" BackgroundColor="White" HeightRequest="1000">
